{
  "name": "react-vite-starter",
  "author": "dickyandeyan",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "dotenvx run -f .env -- vite",
    "build:sandbox": "tsc -b && dotenvx run -f .env.sandbox -- vite build",
    "build": "tsc -b && dotenvx run -f .env.production -- vite build",
    "format": "npx @biomejs/biome format --write ./",
    "lint": "npx @biomejs/biome lint --write ./",
    "preview:sandbox": "dotenvx run -f .env.sandbox -- vite preview",
    "preview": "dotenvx run -f .env.production -- vite preview"
  },
  "dependencies": {
    "@dotenvx/dotenvx": "^1.24.0",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-accordion": "^1.2.1",
    "@radix-ui/react-alert-dialog": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.1",
    "@radix-ui/react-scroll-area": "^1.2.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.1",
    "@tanstack/react-query": "^5.59.13",
    "@tanstack/react-query-devtools": "^5.59.13",
    "@vis.gl/react-google-maps": "^1.5.4",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "cmdk": "1.0.0",
    "crypto-js": "^4.2.0",
    "date-fns": "^4.1.0",
    "jotai": "^2.10.0",
    "ky": "^1.7.2",
    "lucide-react": "^0.452.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.2",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.27.0",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "^1.1.1",
    "zod": "^3.23.8"
